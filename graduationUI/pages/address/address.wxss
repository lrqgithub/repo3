.dw:hover{
	border-radius: 50%;
	box-shadow: 0px 0px 50px 15px rgba(0,0,0,0.3);
	transition: box-shadow 0.5s;
}
.dw{
  box-shadow: 0px 0px 10px 5px rgba(0,0,0,0.3);
  border-radius: 25px;
  background:#ffffff;
  height: 50px;
  width: 50px;
  display: flex;
  flex-flow: row;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 40px;
  right: 40px;
  z-index: 3;
}
.dw image{
display: block;
width: 40px;
}
.palce-holder-nav{
  background: linear-gradient(to right,#17d6a0, #25bcc5);
}
.palce-holder{
  background: linear-gradient(to right,#17d6a0, #25bcc5);
   display: flex;
  flex-flow: row;
  justify-content:space-between;
  align-items: center;
  
}
.place_info{
  color: blue;
  width: 100%;
  /* height: 160rpx; */
  position: fixed;
  bottom: 0px;
}
.icon-houtui{
margin-left: 5rpx;
  box-sizing: border-box;
}
.icon-saoyisao{
  margin-right: 190rpx;
  box-sizing: border-box;
}
.content{
  background:#e6fffc; 
  overflow-y: auto;
}
.top{
  position: relative;
z-index: 2;
  font-size: 25rpx;
  width: 100%;
  background:#fff;
  display: flex;
  flex-flow: wrap;
  justify-content: center;
  align-items: center;
  box-shadow: 0px 20px 20px -20px #5E5E5E;
}
.list_01{
  padding: 15rpx 0;
  width: 95%;

display: flex;

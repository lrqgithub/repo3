}
.list_left{
  display: flex;
  flex-flow: row;
  justify-content: flex-start;
  align-items: center;
}
.list_01 icon{
margin-right: 25rpx;
}
.list_left_01{
  width: 50%;
  display: flex;
  flex-flow: row;
  justify-content: flex-start;
  align-items: center;
  border-right:1px solid #aaaa;
  padding-left: 49rpx;
  box-sizing: border-box;
}
.list_right_01{
  width: 50%;
  display: flex;
  flex-flow: row;
